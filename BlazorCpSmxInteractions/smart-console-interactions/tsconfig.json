{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es2019",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../wwwroot/js"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
